{
  "name": "toolifylab",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "format": "prettier --write .",
    "prepare": "husky",
    "postbuild": "next-sitemap",
    "safe-upgrade": "npx npm-check-updates -u && npm install",
    "test": "jest",
    "test:setup": "npm install --save-dev jest @testing-library/react @testing-library/jest-dom jest-environment-jsdom",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security": "npm audit --audit-level=moderate",
    "pre-commit": "lint-staged",
    "validate": "npm run type-check && npm run lint && npm run build",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse-ci",
    "health-check": "npm audit && npm run type-check && npm run lint"
  },
  "dependencies": {
    "@google/genai": "^1.23.0",
    "@hookform/resolvers": "^5.2.2",
    "@serwist/next": "^9.2.1",
    "@types/qrcode": "^1.5.5",
    "@vercel/analytics": "^1.5.0",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "exifr": "^7.1.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.545.0",
    "motion": "^12.23.22",
    "next": "^15.5.4",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-easy-crop": "^5.5.3",
    "react-hook-form": "^7.64.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.2.1",
    "rehype-highlight": "^7.0.2",
    "remove-markdown": "^0.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/css": "^0.12.0",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/estree": "^1.0.8",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "serwist": "^9.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "typescript-eslint": "^8.46.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore",
          "ci",
          "build",
          "revert"
        ]
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-max-length": [
        2,
        "always",
        100
      ],
      "body-max-line-length": [
        2,
        "always",
        100
      ]
    }
  }
}
